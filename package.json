{
  "name": "jxdsp.com",
  "version": "0.0.1-alpha.0",
  "description": "解析短视频",
  "main": "",
  "scripts": {
    "run_only_on_first_use": "npm run install_nrm_use_taobao && npm run install_global_packages",
    "default": "npm run build",
    "build": "npm run build_composer && npm run build_npm && npm run gulp",
    "build_npm": "npm i",
    "build_composer": "npm run composer_i",
    "gulp": "gulp",
    "dev": "cross-env NODE_ENV=dev && npm run default",
    "clean": "gulp clean",
    "cleanX": "gulp cleanX",
    "lints": "gulp lints",
    "prettier-check": "prettier --check src/webFrontEnd/",
    "prettier-write": "npx prettier --write src/webFrontEnd/",
    "prettier-write-js": "npx prettier --write src/webFrontEnd/js",
    "prettier-write-css": "npx prettier --write src/webFrontEnd/css",
    "prettier-write-html": "npx prettier --write src/webFrontEnd/template",
    "prettier-stylelint-check": "stylelint-config-prettier-check",
    "prettier-eslint-check": "npx eslint-config-prettier src/webFrontEnd/js/site.js",
    "watch": "gulp watch",
    "serve": "gulp serve",
    "composer_i": "composer i",
    "composer_u": "composer u",
    "composer_selfup": "composer self-update",
    "install_nrm_use_taobao": "npm -g i nrm && nrm use taobao && nrm test",
    "install_global_packages": "npm -g i npm-check-updates gulp-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZhangChengLin/jxdsp.com.git"
