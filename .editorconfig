# editorconfig.org

root = true

[*]
charset                  = utf-8
end_of_line              = lf
indent_size              = 4
indent_style             = space
trim_trailing_whitespace = true
insert_final_newline     = true

[/*.json]
indent_size          = 2
insert_final_newline = false

[/src/webFrontEnd/template/**/*.{html,htm}]
insert_final_newline = false

[/gulpfile.js/**/*.js]
indent_size = 2

[/src/webFrontEnd/js/**/*.js]
indent_size = 2
